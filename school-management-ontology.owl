<?xml version="1.0"?>
<rdf:RDF xmlns="http://example.org/ontology/school-management#"
     xml:base="http://example.org/ontology/school-management"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#">

    <!-- =========================
         Ontology header
         ========================= -->
    <owl:Ontology rdf:about="http://example.org/ontology/school-management">
        <rdfs:label xml:lang="en">School Management System Ontology</rdfs:label>
        <rdfs:comment xml:lang="en">
            An OWL 2 ontology for a School Management System (SMS): students, teachers, sections, courses,
            enrollments, timetable, attendance, assessments, grades, fees, and library.
            Works out-of-the-box in Protégé. Use a reasoner (HermiT) to classify and infer.
        </rdfs:comment>
        <owl:versionInfo>1.0.0</owl:versionInfo>
    </owl:Ontology>

    <!-- =========================
         Annotation Properties
         ========================= -->
    <owl:AnnotationProperty rdf:about="#usageNote">
        <rdfs:label xml:lang="en">usage note</rdfs:label>
        <rdfs:comment xml:lang="en">Human-readable note for how/where to use a class/property in the SMS.</rdfs:comment>
    </owl:AnnotationProperty>

    <!-- =========================
         Classes
         ========================= -->

    <!-- Core entities -->
    <owl:Class rdf:about="#Organization">
        <rdfs:label xml:lang="en">Organization</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#School">
        <rdfs:subClassOf rdf:resource="#Organization"/>
        <rdfs:label xml:lang="en">School</rdfs:label>
        <usageNote xml:lang="en">Top-level container for people, academic years, buildings, sections, and policies.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Person">
        <rdfs:label xml:lang="en">Person</rdfs:label>
        <usageNote xml:lang="en">Superclass for all human roles (Student/Teacher/Parent/Staff).</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Student">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label xml:lang="en">Student</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Teacher">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label xml:lang="en">Teacher</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Parent">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label xml:lang="en">Parent/Guardian</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Staff">
        <rdfs:subClassOf rdf:resource="#Person"/>
        <rdfs:label xml:lang="en">Staff</rdfs:label>
    </owl:Class>

    <!-- Academic structure -->
    <owl:Class rdf:about="#AcademicYear">
        <rdfs:label xml:lang="en">Academic Year</rdfs:label>
        <usageNote xml:lang="en">Use to group sections, enrollments, fee accounts; e.g., 2025–2026.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Term">
        <rdfs:label xml:lang="en">Term/Semester</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Subject">
        <rdfs:label xml:lang="en">Subject</rdfs:label>
        <usageNote xml:lang="en">High-level category like Mathematics, English, Physics.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Course">
        <rdfs:label xml:lang="en">Course</rdfs:label>
        <usageNote xml:lang="en">A curriculum unit offered by the school; has a subject and assessments.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#ClassSection">
        <rdfs:label xml:lang="en">Class Section</rdfs:label>
        <usageNote xml:lang="en">A cohort/group (e.g., Grade 10 - Section A) for a specific academic year.</usageNote>
    </owl:Class>

    <!-- N-ary relations / management entities -->
    <owl:Class rdf:about="#Enrollment">
        <rdfs:label xml:lang="en">Enrollment</rdfs:label>
        <usageNote xml:lang="en">N-ary relation: connects Student + ClassSection + AcademicYear (+ date/status).</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#TeachingAssignment">
        <rdfs:label xml:lang="en">Teaching Assignment</rdfs:label>
        <usageNote xml:lang="en">N-ary relation: connects Teacher + Course + ClassSection (+ term/role).</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#ScheduleItem">
        <rdfs:label xml:lang="en">Schedule Item</rdfs:label>
        <usageNote xml:lang="en">Timetable entry for a section/course/teacher in a room (day/time).</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#AttendanceRecord">
        <rdfs:label xml:lang="en">Attendance Record</rdfs:label>
        <usageNote xml:lang="en">Attendance for a student for a specific schedule item/date.</usageNote>
    </owl:Class>

    <!-- Assessment and grading -->
    <owl:Class rdf:about="#Assessment">
        <rdfs:label xml:lang="en">Assessment</rdfs:label>
        <usageNote xml:lang="en">Superclass for Exam/Quiz/Assignment.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Exam">
        <rdfs:subClassOf rdf:resource="#Assessment"/>
        <rdfs:label xml:lang="en">Exam</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Assignment">
        <rdfs:subClassOf rdf:resource="#Assessment"/>
        <rdfs:label xml:lang="en">Assignment</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Submission">
        <rdfs:label xml:lang="en">Submission</rdfs:label>
        <usageNote xml:lang="en">A student's submitted work for an assessment (file/link, timestamp, status).</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#GradeRecord">
        <rdfs:label xml:lang="en">Grade Record</rdfs:label>
        <usageNote xml:lang="en">Stores a student's grade for an assessment (letter/score/remarks).</usageNote>
    </owl:Class>

    <!-- Facilities -->
    <owl:Class rdf:about="#Building">
        <rdfs:label xml:lang="en">Building</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Classroom">
        <rdfs:label xml:lang="en">Classroom</rdfs:label>
    </owl:Class>

    <!-- Fees -->
    <owl:Class rdf:about="#FeeAccount">
        <rdfs:label xml:lang="en">Fee Account</rdfs:label>
        <usageNote xml:lang="en">Per-student account per academic year; contains payments and balance.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#Payment">
        <rdfs:label xml:lang="en">Payment</rdfs:label>
    </owl:Class>

    <!-- Library -->
    <owl:Class rdf:about="#LibraryBook">
        <rdfs:label xml:lang="en">Library Book</rdfs:label>
    </owl:Class>

    <owl:Class rdf:about="#Borrowing">
        <rdfs:label xml:lang="en">Borrowing</rdfs:label>
        <usageNote xml:lang="en">N-ary relation: Student borrows a Book (borrow/return dates, fine).</usageNote>
    </owl:Class>

    <!-- Controlled vocabularies (as classes with enumerations) -->
    <owl:Class rdf:about="#AttendanceStatus">
        <rdfs:label xml:lang="en">Attendance Status</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <owl:NamedIndividual rdf:about="#Present"/>
                    <owl:NamedIndividual rdf:about="#Absent"/>
                    <owl:NamedIndividual rdf:about="#Late"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
        <usageNote xml:lang="en">Use as value set for AttendanceRecord → hasAttendanceStatus.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#GradeLetter">
        <rdfs:label xml:lang="en">Grade Letter</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <owl:NamedIndividual rdf:about="#GradeA"/>
                    <owl:NamedIndividual rdf:about="#GradeB"/>
                    <owl:NamedIndividual rdf:about="#GradeC"/>
                    <owl:NamedIndividual rdf:about="#GradeD"/>
                    <owl:NamedIndividual rdf:about="#GradeF"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <owl:Class rdf:about="#Gender">
        <rdfs:label xml:lang="en">Gender</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <owl:NamedIndividual rdf:about="#Male"/>
                    <owl:NamedIndividual rdf:about="#Female"/>
                    <owl:NamedIndividual rdf:about="#OtherGender"/>
                </owl:oneOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <!-- Inference helper classes -->
    <owl:Class rdf:about="#AbsentAttendanceRecord">
        <rdfs:subClassOf rdf:resource="#AttendanceRecord"/>
        <rdfs:label xml:lang="en">Absent Attendance Record</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#AttendanceRecord"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasAttendanceStatus"/>
                        <owl:hasValue rdf:resource="#Absent"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <owl:Class rdf:about="#AtRiskStudent">
        <rdfs:subClassOf rdf:resource="#Student"/>
        <rdfs:label xml:lang="en">At-Risk Student</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Student"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasAttendanceRecord"/>
                        <owl:someValuesFrom rdf:resource="#AbsentAttendanceRecord"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <usageNote xml:lang="en">Reasoner infers student is AtRiskStudent if they have any AbsentAttendanceRecord.</usageNote>
    </owl:Class>

    <owl:Class rdf:about="#GradeARecord">
        <rdfs:subClassOf rdf:resource="#GradeRecord"/>
        <rdfs:label xml:lang="en">Grade A Record</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#GradeRecord"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGradeLetter"/>
                        <owl:hasValue rdf:resource="#GradeA"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>

    <owl:Class rdf:about="#HonorRollStudent">
        <rdfs:subClassOf rdf:resource="#Student"/>
        <rdfs:label xml:lang="en">Honor Roll Student</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Student"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="#hasGradeRecord"/>
                        <owl:someValuesFrom rdf:resource="#GradeARecord"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <usageNote xml:lang="en">Reasoner infers HonorRollStudent if they have at least one GradeARecord.</usageNote>
    </owl:Class>

    <!-- =========================
         Object Properties
         ========================= -->

    <!-- Membership -->
    <owl:ObjectProperty rdf:about="#hasMember">
        <rdfs:label xml:lang="en">has member</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Person"/>
        <usageNote xml:lang="en">Use to attach any person to the school as a member.</usageNote>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasStudent">
        <rdfs:subPropertyOf rdf:resource="#hasMember"/>
        <rdfs:label xml:lang="en">has student</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTeacher">
        <rdfs:subPropertyOf rdf:resource="#hasMember"/>
        <rdfs:label xml:lang="en">has teacher</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Teacher"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasStaff">
        <rdfs:subPropertyOf rdf:resource="#hasMember"/>
        <rdfs:label xml:lang="en">has staff</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Staff"/>
    </owl:ObjectProperty>

    <!-- Family -->
    <owl:ObjectProperty rdf:about="#hasParent">
        <rdfs:label xml:lang="en">has parent/guardian</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#Parent"/>
        <owl:inverseOf rdf:resource="#isParentOf"/>
        <usageNote xml:lang="en">Use for student-parent relationship; inverse isParentOf.</usageNote>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#isParentOf">
        <rdfs:label xml:lang="en">is parent of</rdfs:label>
        <rdfs:domain rdf:resource="#Parent"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <!-- Academic offering -->
    <owl:ObjectProperty rdf:about="#hasAcademicYear">
        <rdfs:label xml:lang="en">has academic year</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#AcademicYear"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasTerm">
        <rdfs:label xml:lang="en">has term</rdfs:label>
        <rdfs:domain rdf:resource="#AcademicYear"/>
        <rdfs:range rdf:resource="#Term"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#offersCourse">
        <rdfs:label xml:lang="en">offers course</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Course"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#courseHasSubject">
        <rdfs:label xml:lang="en">course has subject</rdfs:label>
        <rdfs:domain rdf:resource="#Course"/>
        <rdfs:range rdf:resource="#Subject"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSection">
        <rdfs:label xml:lang="en">has section</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#ClassSection"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sectionInYear">
        <rdfs:label xml:lang="en">section in academic year</rdfs:label>
        <rdfs:domain rdf:resource="#ClassSection"/>
        <rdfs:range rdf:resource="#AcademicYear"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#sectionOffersCourse">
        <rdfs:label xml:lang="en">section offers course</rdfs:label>
        <rdfs:domain rdf:resource="#ClassSection"/>
        <rdfs:range rdf:resource="#Course"/>
        <usageNote xml:lang="en">Use when a specific section studies a specific course during the year.</usageNote>
    </owl:ObjectProperty>

    <!-- Enrollment (n-ary) -->
    <owl:ObjectProperty rdf:about="#hasEnrollment">
        <rdfs:label xml:lang="en">has enrollment</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#Enrollment"/>
        <usageNote xml:lang="en">Student -> Enrollment. Use enrollmentForStudent/Section/Year inside Enrollment.</usageNote>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enrollmentForStudent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">enrollment for student</rdfs:label>
        <rdfs:domain rdf:resource="#Enrollment"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enrollmentForSection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">enrollment for section</rdfs:label>
        <rdfs:domain rdf:resource="#Enrollment"/>
        <rdfs:range rdf:resource="#ClassSection"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#enrollmentForYear">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">enrollment for academic year</rdfs:label>
        <rdfs:domain rdf:resource="#Enrollment"/>
        <rdfs:range rdf:resource="#AcademicYear"/>
    </owl:ObjectProperty>

    <!-- TeachingAssignment (n-ary) -->
    <owl:ObjectProperty rdf:about="#hasTeachingAssignment">
        <rdfs:label xml:lang="en">has teaching assignment</rdfs:label>
        <rdfs:domain rdf:resource="#Teacher"/>
        <rdfs:range rdf:resource="#TeachingAssignment"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#teachingForTeacher">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">teaching for teacher</rdfs:label>
        <rdfs:domain rdf:resource="#TeachingAssignment"/>
        <rdfs:range rdf:resource="#Teacher"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#teachingForCourse">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">teaching for course</rdfs:label>
        <rdfs:domain rdf:resource="#TeachingAssignment"/>
        <rdfs:range rdf:resource="#Course"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#teachingForSection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">teaching for section</rdfs:label>
        <rdfs:domain rdf:resource="#TeachingAssignment"/>
        <rdfs:range rdf:resource="#ClassSection"/>
    </owl:ObjectProperty>

    <!-- Timetable -->
    <owl:ObjectProperty rdf:about="#hasScheduleItem">
        <rdfs:label xml:lang="en">has schedule item</rdfs:label>
        <rdfs:domain rdf:resource="#ClassSection"/>
        <rdfs:range rdf:resource="#ScheduleItem"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#scheduleForSection">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">schedule for section</rdfs:label>
        <rdfs:domain rdf:resource="#ScheduleItem"/>
        <rdfs:range rdf:resource="#ClassSection"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#scheduleForCourse">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">schedule for course</rdfs:label>
        <rdfs:domain rdf:resource="#ScheduleItem"/>
        <rdfs:range rdf:resource="#Course"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#scheduleForTeacher">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">schedule for teacher</rdfs:label>
        <rdfs:domain rdf:resource="#ScheduleItem"/>
        <rdfs:range rdf:resource="#Teacher"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#scheduleInRoom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">schedule in room</rdfs:label>
        <rdfs:domain rdf:resource="#ScheduleItem"/>
        <rdfs:range rdf:resource="#Classroom"/>
    </owl:ObjectProperty>

    <!-- Attendance -->
    <owl:ObjectProperty rdf:about="#hasAttendanceRecord">
        <rdfs:label xml:lang="en">has attendance record</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#AttendanceRecord"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attendanceForStudent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">attendance for student</rdfs:label>
        <rdfs:domain rdf:resource="#AttendanceRecord"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#attendanceForScheduleItem">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">attendance for schedule item</rdfs:label>
        <rdfs:domain rdf:resource="#AttendanceRecord"/>
        <rdfs:range rdf:resource="#ScheduleItem"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasAttendanceStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">has attendance status</rdfs:label>
        <rdfs:domain rdf:resource="#AttendanceRecord"/>
        <rdfs:range rdf:resource="#AttendanceStatus"/>
    </owl:ObjectProperty>

    <!-- Assessment and grades -->
    <owl:ObjectProperty rdf:about="#courseHasAssessment">
        <rdfs:label xml:lang="en">course has assessment</rdfs:label>
        <rdfs:domain rdf:resource="#Course"/>
        <rdfs:range rdf:resource="#Assessment"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#submissionForStudent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">submission for student</rdfs:label>
        <rdfs:domain rdf:resource="#Submission"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#submissionForAssessment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">submission for assessment</rdfs:label>
        <rdfs:domain rdf:resource="#Submission"/>
        <rdfs:range rdf:resource="#Assessment"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasSubmission">
        <rdfs:label xml:lang="en">has submission</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#Submission"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasGradeRecord">
        <rdfs:label xml:lang="en">has grade record</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#GradeRecord"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#gradeRecordForStudent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">grade record for student</rdfs:label>
        <rdfs:domain rdf:resource="#GradeRecord"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#gradeRecordForAssessment">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">grade record for assessment</rdfs:label>
        <rdfs:domain rdf:resource="#GradeRecord"/>
        <rdfs:range rdf:resource="#Assessment"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasGradeLetter">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">has grade letter</rdfs:label>
        <rdfs:domain rdf:resource="#GradeRecord"/>
        <rdfs:range rdf:resource="#GradeLetter"/>
    </owl:ObjectProperty>

    <!-- Facilities -->
    <owl:ObjectProperty rdf:about="#hasBuilding">
        <rdfs:label xml:lang="en">has building</rdfs:label>
        <rdfs:domain rdf:resource="#School"/>
        <rdfs:range rdf:resource="#Building"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#hasRoom">
        <rdfs:label xml:lang="en">has room</rdfs:label>
        <rdfs:domain rdf:resource="#Building"/>
        <rdfs:range rdf:resource="#Classroom"/>
    </owl:ObjectProperty>

    <!-- Fees -->
    <owl:ObjectProperty rdf:about="#hasFeeAccount">
        <rdfs:label xml:lang="en">has fee account</rdfs:label>
        <rdfs:domain rdf:resource="#Student"/>
        <rdfs:range rdf:resource="#FeeAccount"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#feeAccountForStudent">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">fee account for student</rdfs:label>
        <rdfs:domain rdf:resource="#FeeAccount"/>
        <rdfs:range rdf:resource="#Student"/>
    </owl:ObjectProperty>

    <owl:ObjectProperty rdf:about="#feeAccountForYear">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:label xml:lang="en">fee account for year</rdfs:label>
        <rdfs:domain rdf:resource="#FeeAccount"/>
        <rdfs:range rdf:resource="#AcademicYear"/>
