                <owl:onClass rdf:resource="#Assessment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasGradeLetter"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#GradeLetter"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- Borrowing must have exactly 1 Student and 1 Book -->
    <owl:Class rdf:about="#Borrowing">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#borrowingForStudent"/>
