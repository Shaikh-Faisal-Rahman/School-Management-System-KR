    <!-- =========================
         Constraints (Cardinality)
         ========================= -->
    <!-- Enrollment must have exactly 1 Student, 1 Section, 1 AcademicYear -->
    <owl:Class rdf:about="#Enrollment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#enrollmentForStudent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#Student"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#enrollmentForSection"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#ClassSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#enrollmentForYear"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#AcademicYear"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- TeachingAssignment must have exactly 1 Teacher, 1 Course, 1 Section -->
    <owl:Class rdf:about="#TeachingAssignment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#teachingForTeacher"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#Teacher"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#teachingForCourse"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#Course"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#teachingForSection"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#ClassSection"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- AttendanceRecord must have exactly 1 Student, 1 ScheduleItem, 1 Status -->
    <owl:Class rdf:about="#AttendanceRecord">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#attendanceForStudent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#Student"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#attendanceForScheduleItem"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#ScheduleItem"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#hasAttendanceStatus"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#AttendanceStatus"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>

    <!-- GradeRecord must have exactly 1 Student, 1 Assessment, 1 GradeLetter -->
    <owl:Class rdf:about="#GradeRecord">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="#gradeRecordForStudent"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="#Student"/>
